{
	"name": "backend",
	"private": true,
	"type": "module",
	"packageManager": "yarn@4.6.0",
	"scripts": {
		"generate": "graphql-codegen && prisma generate",
		"gen:watch": "graphql-codegen --watch",
		"sync-db": "prisma db push",
		"dev": "tsx watch src/server.ts",
		"typecheck": "tsc --noEmit",
		"build": "tsc -p .",
		"start": "node dist/server.js",
		"init-db": "docker exec -it mongo mongosh --eval 'rs.initiate({_id:\"rs0\", members:[{_id:0, host:\"localhost:27017\"}]})'"
	},
	"dependencies": {
		"@azure/identity": "4.12.0",
		"@azure/storage-blob": "12.28.0",
		"@envelop/graphql-middleware": "8.0.0",
		"@graphql-yoga/plugin-jwt": "3.9.1",
		"@js-joda/core": "5.6.5",
		"@microsoft/microsoft-graph-client": "3.0.7",
		"@package/shared": "workspace:*",
		"@prisma/client": "6.15.0",
		"argon2": "0.44.0",
		"graphql": "16.11.0",
		"graphql-middleware": "6.1.35",
		"graphql-scalars": "1.24.2",
		"graphql-shield": "7.6.5",
		"graphql-yoga": "5.15.1",
		"jose": "6.0.12",
		"jsonwebtoken": "9.0.2",
		"pdfkit": "0.17.2",
		"prisma": "6.15.0",
		"ts-pattern": "5.8.0"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "5.0.7",
		"@graphql-codegen/typescript": "4.1.6",
		"@graphql-codegen/typescript-resolvers": "4.5.1",
		"@graphql-tools/load-files": "7.0.1",
		"@graphql-tools/merge": "9.1.1",
		"@graphql-tools/schema": "10.0.25",
		"@microsoft/microsoft-graph-types": "2.40.0",
		"@types/jsonwebtoken": "9",
		"@types/node": "24.2.1",
		"@types/pdfkit": "0.17.3",
		"tsx": "4.20.4",
		"typescript": "5.9.2"
	}
}
